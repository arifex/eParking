<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient
                android:startColor="@color/btn_backColor"
                android:endColor="@color/btn_backColor"
                android:angle="270"/>
            <stroke
                android:width="1dp"
                android:color="@color/btn_Stroke" />
            <corners android:radius="0dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="@color/btn_backClick"
                android:endColor="@color/btn_backClick"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="@color/btn_Stroke" />
            <corners android:radius="0dp"/>

        </shape>
    </item>
</selector>